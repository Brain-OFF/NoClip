<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="850.0" style="-fx-background-color: #F1F7ED;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.IndexController">
   <children>
      <Pane layoutX="155.0" layoutY="14.0" prefHeight="573.0" prefWidth="682.0" style="-fx-border-color: #0000FF#0000FF; -fx-border-width: 3px; -fx-border-radius: 6px; -fx-background-color: #F1F7ED;">
         <children>
            <TableView fx:id="commandes" layoutX="14.0" layoutY="126.0" onMouseClicked="#SelectItem" prefHeight="377.0" prefWidth="660.0" stylesheets="@../../style.css">
              <columns>
                <TableColumn fx:id="nom" prefWidth="56.0" text="Nom" />
                <TableColumn fx:id="prenom" prefWidth="167.0" text="Prenom" />
                  <TableColumn fx:id="adressecomplet" prefWidth="133.0" text="AdresseComplet" />
                  <TableColumn fx:id="telephone" prefWidth="84.0" text="Telephone" />
                  <TableColumn fx:id="email" prefWidth="94.0" text="Email" />
                  <TableColumn id="prixtotal" fx:id="prixtotal" prefWidth="75.0" text="Prix total" />
                
              </columns>
            </TableView>
            <Label alignment="CENTER" layoutX="14.0" layoutY="16.0" prefHeight="49.0" prefWidth="660.0" text="Liste des commandes" textFill="#393d3f">
               <font>
                  <Font name="Times New Roman Bold" size="28.0" />
               </font>
            </Label>
            <Button layoutX="136.0" layoutY="523.0" mnemonicParsing="false" onAction="#NewCommande" prefHeight="31.0" prefWidth="249.0" style="-fx-background-color: #ADD8E6;" text="+ Ajouter une nouvelle commande" textFill="#222020">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </Button>
            <Button layoutX="401.0" layoutY="523.0" mnemonicParsing="false" onAction="#Delete" prefHeight="31.0" prefWidth="111.0" style="-fx-background-color: #ADD8E6;" text="Supprimer" textFill="#0d0c0c">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </Button>
            <Button layoutX="528.0" layoutY="523.0" mnemonicParsing="false" onAction="#Edit" prefHeight="31.0" prefWidth="127.0" style="-fx-background-color: #ADD8E6;" text="Modifier" textFill="#111111">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="tfrecherche" layoutX="141.0" layoutY="73.0" onKeyReleased="#Search" prefHeight="31.0" prefWidth="531.0" promptText="Rechercher une commande">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </TextField>
         </children>
      </Pane>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="573.0" prefWidth="129.0" style="-fx-border-color: #0000FF#0000FF; -fx-border-width: 3px; -fx-border-radius: 6px; -fx-background-color: #F1F7ED;">
         <children>
            <Button fx:id="Commandes" mnemonicParsing="false" onAction="#showCommande" prefHeight="30.0" prefWidth="176.0" style="-fx-border-radius: 10px; -fx-background-radius: 10px; -fx-background-color: #ADD8E6;" text="Commandes" textFill="#393d3f">
               <font>
                  <Font name="Times New Roman" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="3.0" top="3.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
